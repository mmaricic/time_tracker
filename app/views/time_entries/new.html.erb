<% @time_entry.errors.full_messages.each do |error| %>
  <div><%= error %></div>
<% end %>

<%= form_with model: @time_entry, local: true do |form| %>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div>
    <%= form.label :start_time %>
    <%= form.datetime_field :start_time %>
  </div>
  <div>
    <%= form.label :end_time %>
    <%= form.datetime_field :end_time %>
  </div>
  <%= form.hidden_field :manual, value: true %>
  <%= form.submit "Create" %>
<% end %>